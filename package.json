{
  "name": "web-chat",
  "version": "0.0.1",
  "description": "create web chat",
  "author": "Timerbayev Aynur <timerbayevaf@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .tsx,.ts",
    "stylelint": "npx stylelint '**/*.css'",
    "dev": "parcel static/index.html --no-source-maps --dist-dir ./build",
    "build": "parcel build static/index.html --no-source-maps --dist-dir ./build",
    "server": "node ./server.js",
    "start": "yarn build && yarn server"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.2",
    "@parcel/transformer-typescript-tsc": "2.8.2",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "csstype": "3.1.1",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "parcel": "2.8.2",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "typescript": ">=3.0.0"
  },
  "alias": {
    "components": "./src/components",
    "core": "./src/core",
    "modules": "./src/modules",
    "pages": "./src/pages",
    "utils": "./src/utils",
    "types": "./src/types",
    "constants": "./src/constants",
    "mocks": "./src/mocks"
  },
  "dependencies": {
    "express": "4.18.2"
  }
}
